{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    //  "baseUrl": "src",
    // // "paths": {
    // //   "*": ["./src/*"]
    // // },
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.js", "src/**/*.ts", "src/**/*.json"],
  "exclude": ["node_modules", "build"]
}